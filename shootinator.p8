function create_pbul(_x,_y,dx)
	dx=dx or 0
	local bul=create_mob(17,_x,_y)
	local _ENV = bul
	pow=1
	dy=-2
	fra=rnd(27)\1
	function upd(_ENV)
		self=_ENV
		x+=dx
		fra+=1
		fra=fra%27
		if x<-8 or	x>132 
		or	y<-8 or	y>132 then
				remove(self)
		end
	end
	bul.olddraw=draw
	function draw()
		local f=fra\9
		if f==0 then
			pal(12,7)
			pal(13,12)
		elseif f==1 then
		else
			pal(1,12)
			pal(12,7)
			pal(13,7)
		end
		bul:olddraw()
		pal()
	end
	add(mobs,bul)
	add(buls,bul)
end

