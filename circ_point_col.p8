function _init()
	points={}
	for i=1,1000 do
	add(points, {rnd(128),rnd(128)})
	end
end

function _update60()
points={}
	for i=1,1000 do
	add(points, {rnd(128),rnd(128)})
	end
end

function _draw()
	cls()
	for p in all(points) do
		local c=7
		local rx,ry=64-p[1],64-p[2]
		if rx<32
			and ry<32 then
			c=3
		end
		if rx*rx+ry*ry<1024 then
			c=2
		end
		
		pset(p[1],p[2],c)
	end
end
